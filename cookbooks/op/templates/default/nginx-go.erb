server {
        listen 443;
        server_name mystictriad.com mystictriadgames.com;
        root /home/go/Go/www;

        ssl on;
        ssl_certificate /etc/nginx/certs/go.mut8ed.com.cert;
        ssl_certificate_key /etc/nginx/certs/go.mut8ed.com.key;
        ssl_session_timeout 30m;

        gzip  on;
        gzip_http_version 1.1;
        gzip_vary on;
        gzip_comp_level 6;
        gzip_proxied any;
        gzip_types text/plain text/css application/json application/x-javascript text/xml application/xml application/xml+rss text/javascript;
}
